<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane fx:id="gridPane" alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.Main.MainView">
    <children>
        <VBox alignment="TOP_CENTER" prefHeight="600.0" prefWidth="400.0" style="-fx-background-color: #EFEFEF;">
            <children>
                <Label prefWidth="315.0" text="首頁" textAlignment="JUSTIFY">
                    <font>
                        <Font name="Microsoft JhengHei Bold" size="20.0" />
                    </font>
                    <VBox.margin>
                        <Insets bottom="20.0" />
                    </VBox.margin>
                </Label>
                <HBox alignment="CENTER_LEFT" layoutX="60.0" layoutY="60.0" prefHeight="50.0" prefWidth="200.0">
                    <children>
                        <Label text="登入帳號：">
                            <HBox.margin>
                                <Insets right="15.0" />
                            </HBox.margin>
                        </Label>
                        <Label fx:id="promptLabel" layoutX="10.0" layoutY="28.0" style="-fx-background-color: DEDEDE;">
                            <padding>
                                <Insets left="5.0" right="5.0" />
                            </padding></Label>
                    </children>
                </HBox>
            <Button fx:id="logoutBtn" mnemonicParsing="false" text="登出" />
            </children>
            <padding>
                <Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
            </padding>
            <GridPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </GridPane.margin>
        </VBox>
    </children>
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
</GridPane>
